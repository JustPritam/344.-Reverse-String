class Solution {
    public void reverseString(char[] s) {
        char t;
        int j=s.length-1;
        for(int i=0;i<s.length;i++)
        {
            if(i==s.length/2)
                break;
            else
            {
                t=s[i];
                s[i]=s[j];
                s[j]=t;
                j--;
            }
        }
    }
}
